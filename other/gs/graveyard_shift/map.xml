<map proto="1.5.0" min-server-version="1.21.8">
<name>Graveyard Shift</name>
<version>1.0</version>
<objective>Jump into the enemy team's portal, located below their spawn point!</objective>
<created>2025-07-22</created>
<include id="gs"/>
<authors>
    <author uuid="9d4779a8-d35f-45d2-b51c-95de7a77e128"/> <!-- zzuf -->
</authors>
<terrain environment="nether"/>
<teams>
    <team id="yellow-team" color="yellow" max="10">Yellow</team>
    <team id="red-team" color="dark red" max="10">Red</team>
</teams>
<spawns>
    <spawn team="red-team" yaw="90">
        <region>
            <point>23,65,2</point>
        </region>
    </spawn>
    <spawn team="yellow-team" yaw="-90">
        <region>
            <point>-42,65,2</point>
        </region>
    </spawn>
    <default>
        <regions>
            <point>-9.5,85,-39.5</point>
            <point yaw="180">-9.5,85,43.5</point>
        </regions>
    </default>
</spawns>
